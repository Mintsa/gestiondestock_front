    "gs-api:create-dist": "mkdir -p tools/swagger/dist && mkdir -p tools/swagger/src",
    "gs-api:download": "cp /home/romuald-mba-mintsa/PostDev/WorkSpace/gestiondestock_src/target/gestiondestock-0.0.1-SNAPSHOT.jar ./tools/swagger/dist && cp /home/romuald-mba-mintsa/PostDev/WorkSpace/gestiondestock_src/target/swagger.json ./tools/swagger/src",
    "gs-api:extract": "node ./tools/swagger/src/swagger.json 0.0.1-SNAPSHOT",
    "gs-api:swagger-gen": "./node_modules/.bin/ng-swagger-gen -i ./tools/swagger/src/swagger.json -o src/gs-api/src"
  },
  "private": true,

    "@ng-bootstrap/ng-bootstrap": "^14.2.0",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.5",
    "font-awesome": "^4.7.0",
    "rxjs": "~7.5.0",
